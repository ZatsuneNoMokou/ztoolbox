{
	"applications": {
		"gecko": {
			"id": "ztoolbox_dev@zatsunenomokou.eu",
			"update_url": "https://gitlab.com/ZatsuneNoMokou/ztoolbox/raw/master/dist/z_toolbox_dev.update.json",
			"strict_min_version": "91.0"
		}
	},
	"manifest_version": 3,
	"name": "Z-ToolBox (Dev)",
	"short_name": "ZToolBoxDev",
	"description": "__MSG_extShortDesc__",
	"author": "Zatsune No Mokou",
	"version": "0.20.2",
	"icons": {
		"16": "/icon_16.png",
		"48": "/icon_48.png",
		"96": "/icon_96.png",
		"128": "/icon_128.png"
	},
	"background": {
		"service_worker": "/data/js/index.js",
		"type": "module"
	},
	"action": {
		"browser_style": false,
		"default_icon": {
			"16": "/icon_16.png",
			"48": "/icon_48.png",
			"96": "/icon_96.png",
			"128": "/icon_128.png"
		},
		"default_popup": "/data/panel.html"
	},
	"content_scripts": [
		{
			"matches": ["*://ppa.launchpad.net/*/*/*/"],
			"js": ["/data/js/contentscripts/launchpad-addlink.js"]
		},
		{
			"matches": ["https://iqdb.org/"],
			"js": ["/data/js/contentscripts/iqdb_search.js"],
			"run_at": "document_start"
		}
	],
	"default_locale": "en",
	"permissions": [
		"alarms",
		"contextMenus",
		"notifications",
		"scripting",
		"storage",
		"tabs",
		"activeTab",
		"nativeMessaging"
	],
	"optional_permissions": [
		"clipboardWrite"
	],
	"host_permissions": [
		"<all_urls>",
		"http://*/*",
		"https://*/*"
	],
	"options_ui": {
		"page": "data/options.html"
	},
	"minimum_chrome_version": "92.0"
}
