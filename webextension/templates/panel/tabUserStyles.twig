{%- for item in items -%}
<div class="buttonItem cursor">
	{%- if not item.data -%}
		<span class="icon material-symbols-outlined">tab</span>
	{%- else -%}
		<input class="icon" type="checkbox" name="{{ item.data.id }}" id="{{ item.data.id }}" readonly disabled{% if item.data.enabled %} checked{% endif %} />
	{%- endif -%}

	<label class="title ellipse" for="{{ item.data.id }}">{{item.title}}</label>
	{%- if not item.data -%}
		<div class="data ellipse">Aucun style</div>
	{%- else -%}
		<div class="data ellipse">
			<label for="{{ item.data.id }}">{{ item.data.label }}</label>
		</div>
	{%- endif -%}
</div>
{%- endfor -%}
